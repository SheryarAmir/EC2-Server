# Copy this file to .env and replace the values with your bucket name and region.
# On Windows: copy env.template .env
# On Mac/Linux: cp env.template .env

# S3 bucket for image uploads (required for /api/upload)
S3_BUCKET=your-bucket-name

# AWS region where the bucket lives (e.g. us-east-1, eu-west-1)
AWS_REGION=us-east-1

# Optional: server port (Next.js default is 3000)
# PORT=3000
